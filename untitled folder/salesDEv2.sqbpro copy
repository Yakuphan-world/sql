SELECT  employee.empName , max(sales.salesPrice) FROM sales  JOIN employee ON  sales.empId= employee.empId  JOIN  brand ON  sales.brandID=brand.brandId where employee.empName(&quot;Yakup&quot;);</sql><current_tab id="0"/></tab_sql></sqlb_project>
